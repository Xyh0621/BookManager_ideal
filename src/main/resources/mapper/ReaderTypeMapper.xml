<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<!--
    名称空间
-->
<mapper namespace="mapper.ReaderTypeMapper">
    <select id="selectAll" resultType="pojo.ReaderType">
    select * from tb_readertype;
    </select>


    <select id="selectByrdTypeID" resultType="pojo.ReaderType">
        select * from tb_readertype where rdType = #{rdTypeID}
    </select>
</mapper>